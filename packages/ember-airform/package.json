  "license": "MIT",
  "author": "CJ Patoilo <cjpatoilo@gmail.com>",
  "private": false,
  "main": "dist/ember-airform.cjs.js",
  "module": "dist/ember-airform.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
